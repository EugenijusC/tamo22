{
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "error",
      "semi": "error",
      "no-unused-vars": "error",
      "indent": ["error", 2],
      "no-trailing-spaces": "error",
      "quotes": ["error", "single"],
      "max-len": ["error", { "code": 80 }],
      "no-multiple-empty-lines": ["error", { "max": 1 }]
    }
  }
  